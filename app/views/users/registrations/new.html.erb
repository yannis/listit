<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "users/shared/error_messages", resource: resource %>

  <div>
    <%= f.email_field :email, autofocus: true, placeholder: "E-mail", autocomplete: "email", required: true %>
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<p class="">
  or
  <br><%= link_to "Log in", new_session_path(resource_name) %>
</p>
